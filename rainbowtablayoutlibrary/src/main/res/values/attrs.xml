<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="RainbowTabLayout">
        <attr name="rtl_tabLine" format="boolean" />
        <attr name="rtl_tabLinePosition" format="enum">
            <enum name="TOP" value="0"/>
            <enum name="BOTTOM" value="1"/>
        </attr>
        <attr name="rtl_tabIndicator" format="boolean" />
        <attr name="rtl_tabIndicatorPosition" format="enum">
            <enum name="TOP" value="0"/>
            <enum name="BOTTOM" value="1"/>
            <enum name="ALL" value="1"/>
        </attr>
        <attr name="rtl_tabSeparator" format="boolean" />
        <attr name="rtl_titleBlend" format="boolean" />
        <attr name="rtl_tabMinWidthByMax" format="boolean" />
        <attr name="rtl_distributeEvenly" format="boolean" />
        <attr name="rtl_fontFamily" format="reference" />
        <attr name="rtl_tabViewPadding" format="dimension" />
        <attr name="rtl_tabViewTextSize" format="dimension" />
        <attr name="rtl_titleColor" format="color" />
    </declare-styleable>
</resources>